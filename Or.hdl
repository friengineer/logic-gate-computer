/**
 * Or gate:
 * result = 1 if (x==1 or y==1),
 *          0 otherwise
 */

CHIP Or {
    IN x, y;
    OUT out;

    PARTS:
    Nand(x=x,y=x,out=a);
    Nand(x=y,y=y,out=b);
    Nand(x=a,y=b,out=out);
}

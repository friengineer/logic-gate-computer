/**
 * And gate:
 * out = 1 if (x == 1 and y == 1)
 *          0 otherwise
 */

CHIP And {
    IN x, y;
    OUT out;

    PARTS:
    Nand(x=x,y=y,out=a);
    Nand(x=a,y=a,out=out);
}

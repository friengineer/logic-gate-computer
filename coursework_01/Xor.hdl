/**
 *  Exclusive-or gate:
 *  out = not ( x == y ).
 */

CHIP Xor {
    IN x, y;
    OUT out;

    PARTS:
    And(x=x,y=y,out=a);
    Or(x=x,y=y,out=b);
    Not(x=a,out=c);
    And(x=b,y=c,out=out);
}

/**
 * Multiplexor. If sel==1 then out = y else out = x.
 */

CHIP Mux {
    IN x, y, sel;
    OUT out;

    PARTS:
    Not(x=sel,out=a);
    And(x=x,y=a,out=b);
    And(x=y,y=sel,out=c);
    Or(x=b,y=c,out=out);
}

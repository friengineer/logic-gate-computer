/**
 * Demultiplexor.
 * {x,y} = {in,0} if sel == 0
 *         {0,in} if sel == 1
 */

CHIP DMux {
    IN in, sel;
    OUT x, y;

    PARTS:
    Not(x=sel,out=a);
    And(x=in,y=a,out=x);
    And(x=in,y=sel,out=y);
}
